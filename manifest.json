{
"manifest_version": 2,
    "name": "The Extension Of Perpetual NFT Exploration",
    "description": "Shows you someone's NFTs when you visit their profile on twitter. It uses HumanityDAO for the twitter => wallet mapping.",
    "version": "0.1112",
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
              "css.css"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
      "16": "icons/icon16.png",
       "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
}
